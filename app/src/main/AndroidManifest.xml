<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.momonyan.app_zyanken_joke">

    <application
        android:allowBackup="false"
        android:icon="@drawable/unnamed-1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/unnamed-1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ChoiceScreen"
            android:label="@string/first_choice" />
        <activity
            android:name=".QuestionScreen"
            android:label="@string/first_question" />
        <activity
            android:name=".ResultScreen"
            android:label="@string/result" />
    </application>

</manifest>